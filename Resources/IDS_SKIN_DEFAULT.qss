/*=============QCheckBox============*/

QCheckBox::indicator:!enabled:unchecked     { image: url(./skin/unsel_check_box_disabled.png);     }
QCheckBox::indicator:!enabled:checked       { image: url(./skin/checkbox_checked_disabled.png);           }
QCheckBox::indicator:unchecked              { image: url(./skin/unsel_check_box_unchecked.png);     }
QCheckBox::indicator:unchecked:hover        { image: url(./skin/unsel_check_box_hover.png);     }
QCheckBox::indicator:unchecked:selected     { image: url(./skin/unsel_check_box_hover.png);     }
QCheckBox::indicator:checked                { image: url(./skin/checkbox_checked.png);           }
QCheckBox::indicator:checked:hover          { image: url(./skin/checkbox_hover.png);           }
QCheckBox::indicator:checked:pressed        { image: url(./skin/checkbox_hover.png);           }
QCheckBox::indicator:indeterminate:hover    { image: url(./skin/unsel_check_box_hover.png);     }
QCheckBox::indicator:indeterminate:selected { image: url(./skin/checkbox_hover.png);           }

 
/*=============QRadioButton============*/

QRadioButton::indicator:!enabled:unchecked  { image: url(./skin/radio_button_disabled.png);       }
QRadioButton::indicator:!enabled:checked    { image: url(./skin/sel_radio_button_disabled.png);   }
QRadioButton::indicator:unchecked           { image: url(./skin/radio_button_normal.png);           }
QRadioButton::indicator:unchecked:hover     { image: url(./skin/radio_button_hover.png);       }
QRadioButton::indicator:unchecked:pressed   { image: url(./skin/radio_button_hover.png);       }
QRadioButton::indicator::checked            { image: url(./skin/sel_radio_button_normal.png);       }
QRadioButton::indicator:checked:hover 	    { image: url(./skin/sel_radio_button_hover.png);   }
QRadioButton::indicator:checked:pressed     { image: url(./skin/sel_radio_button_hover.png);   }
 
 /*=============QScrollBar Horizontal ============*/
  QScrollBar::horizontal {
     /* background: rgb(221, 241, 255);      */
     background-image: url("./skin/uiplugin_bg.png");
     height: 18px;
     margin: 0px 18px 0px 18px;
 }
 
 
 QScrollBar::handle:horizontal {
     border-image: url(./skin/scrollbar_horizontal_normal.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-width: 20px;
 }
 
 QScrollBar::handle:horizontal:hover {
     border-image: url(./skin/scrollbar_horizontal_hover.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-width: 20px;
 }
 
 QScrollBar::handle:horizontal:pressed {
     border-image: url(./skin/scrollbar_horizontal_press.png) 2 2 2 2;
     border-top-width: 1px;
     border-right-width: 1px;
     border-bottom-width: 1px;
     border-left-width: 1px;
     min-width: 20px;
 }
  

 QScrollBar::add-line:horizontal {
     background: rgb(92, 162, 213);
     width: 18px;
     subcontrol-position: right;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:horizontal {
     background: rgb(92, 162, 213);
     width: 18px;     
     subcontrol-position: left;
     subcontrol-origin: margin;
 }

 /* QScrollBar:left-arrow:horizontal:!enabled  { border-image: url(./skin/popup_controls_button_left_gray.png);  }*/
 QScrollBar:left-arrow:horizontal { border-image: url(./skin/popup_controls_button_left.png);  }
 QScrollBar:left-arrow:horizontal:hover { border-image: url(./skin/popup_controls_button_left_hover.png);   }
 /* QScrollBar:right-arrow:horizontal:!enabled { border-image: url(./skin/popup_controls_button_right_gray.png);   } */
 QScrollBar:right-arrow:horizontal { border-image: url(./skin/popup_controls_button_right.png);   }
 QScrollBar:right-arrow:horizontal:hover { border-image: url(./skin/popup_controls_button_right_hover.png);   }
 
 /*=============QScrollBar Vertical ============*/
QScrollBar::vertical {
     background: rgb(221, 241, 255);
     width: 18px;
     margin: 18px 0px 18px 0px;
 }
 
 QScrollBar::handle:vertical {
     border-image: url(./skin/scrollbar_vertical_normal.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-height: 20px;
 }
 QScrollBar::handle:vertical:hover {
     border-image: url(./skin/scrollbar_vertical_hover.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-height: 20px;
 }
 
 QScrollBar::handle:vertical:pressed {
     border-image: url(./skin/scrollbar_vertical_press.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px;
     min-height: 20px;
 }
  
 QScrollBar::add-line:vertical {
     background: rgb(92, 162, 213);
     height: 18px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:vertical {
     background: rgb(92, 162, 213);
     height: 18px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }

 
QScrollBar:up-arrow:vertical 			{ border-image: url(./skin/popup_controls_button_up.png);   }
QScrollBar:up-arrow:vertical:hover 		{ border-image: url(./skin/popup_controls_button_up_hover.png);   }
QScrollBar:down-arrow:vertical 			{ border-image: url(./skin/popup_controls_button_down.png);   }
QScrollBar:down-arrow:vertical:hover 	{ border-image: url(./skin/popup_controls_button_down_hover.png);   }

