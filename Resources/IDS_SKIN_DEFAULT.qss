/* ========== QMenuBar ========== */
QMenuBar, QMenuBar::item {
    background-image: url(./skin/menu_bg.png);
    color: white;
}
QMenuBar::item:selected { border-image: url(./skin/menu_bar_button_hover.png); }
QMenuBar::item:pressed { border-image: url(./skin/menu_bar_button_sel.png); }

/* ========== QMenu ========== */

QMenu {
    border-image: url(./skin/tree_widget_bg_line_box.png) 2 2 2 2;
    border-top-width: 1px transparent;
    border-right-width: 1px transparent;
    border-bottom-width: 1px transparent;
    border-left-width: 1px transparent;
    background-color: white;
    /* margin: 2px; some spacing around the menu */
}

QMenu::item 
{
    padding: 2px 25px 2px 20px;
    border: 1px solid transparent; /* reserve space for selection border */
}


QMenu::item:selected 	{ background-image: url(./skin/listsel.png); 	color: white; }
QMenu::indicator 		{ image: url(./skin/menu_check.png); }
QMenu::right-arrow		{ image: url(./skin/menu_expand.png); }

/* Ashwin: Begin BT1725 fix - Disabled Menu Items in the Right Click Menu doesnt appear gray in Mac */
QMenu::item:!enabled {color:gray; }
/* Ashwin: End BT1725 fix - Disabled Menu Items in the Right Click Menu doesnt appear gray in Mac */

 /* ========== QHeaderView ==========    */ 
